{
    "manifest_version": 2,

    "name": "Page Scroller",
    "description": "This extension scrolls a web page to the bottom then returns to the top",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

    "background": {
        "scripts": ["js/jquery-3.1.1.min.js", "js/script-execution.js"]
    },

    "permissions": [
        "activeTab"
    ]
}